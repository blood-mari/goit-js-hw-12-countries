{{#each this}}
<div class="content_country">
    <div class="country_wrapper">
        <p class="country_title">{{name}}</p>
        <p class="country_desc">Capital: {{capital}}</p>
        <p class="country_desc">Population: {{population}}</p>
        <p class="country_desc">Languages: </p>
            <ul class="country_language">
                {{#each languages}}
                <li>{{name}}</li>
                {{/each}}
            </ul>
    </div>
    <img width= "400px" src="{{flag}}" alt="{{name}}" class="country_img">

</div>
{{/each}}